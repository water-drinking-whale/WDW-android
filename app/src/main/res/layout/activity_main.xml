<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="50dp"
        android:text="메인화면"
        android:textSize="40sp" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/logFab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/mainFab"
        app:layout_constraintEnd_toEndOf="@+id/mainFab"
        app:layout_constraintStart_toStartOf="@+id/mainFab"
        app:layout_constraintTop_toTopOf="@+id/mainFab"
        android:backgroundTint="#ffffff"
        app:fabSize="normal"
        android:src="@drawable/bottom_log"
         />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/noticeFab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/mainFab"
        app:layout_constraintEnd_toEndOf="@+id/mainFab"
        app:layout_constraintStart_toStartOf="@+id/mainFab"
        app:layout_constraintTop_toTopOf="@+id/mainFab"
        android:backgroundTint="#ffffff"
        app:fabSize="normal"
        android:src="@drawable/bottom_notice"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/awardFab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/mainFab"
        app:layout_constraintEnd_toEndOf="@+id/mainFab"
        app:layout_constraintStart_toStartOf="@+id/mainFab"
        app:layout_constraintTop_toTopOf="@+id/mainFab"
        android:backgroundTint="#ffffff"
        app:fabSize="normal"
        android:src="@drawable/award"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/myPageFab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/mainFab"
        app:layout_constraintEnd_toEndOf="@+id/mainFab"
        app:layout_constraintStart_toStartOf="@+id/mainFab"
        app:layout_constraintTop_toTopOf="@+id/mainFab"
        android:backgroundTint="#ffffff"
        app:fabSize="normal"
        android:src="@drawable/my_page"
        />




    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/mainFab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_input_add" />


</RelativeLayout>